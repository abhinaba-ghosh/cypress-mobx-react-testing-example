{
    "video": false,
    "screenshotOnRunFailure": false,
    "component": {
        "testFiles": "**/*.spec.{js,ts,jsx,tsx}",
        "componentFolder": "src"
    },
    "env": {
        "cypress-react-selector": {
            "root": "#__cy_root"
        },
        "cypress-plugin-snapshots": {}
    },
    "ignoreTestFiles": ["**/__snapshots__/*", "**/__image_snapshots__/*"]
}
